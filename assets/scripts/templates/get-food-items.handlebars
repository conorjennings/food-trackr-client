<table class="pure-table pure-table-horizontal">
  <thead>
    <tr>
      <th>Food Name</th>
      <th>Days To Exp</th>
      <th>Remove</th>
      <th>Move To Grocery List</th>
    </tr>
  </thead>

  {{#each items as |item|}}

    <tbody>
      <td>{{item.food_name}}</td>
      <td>{{item.exp_date}}</td>
      <td><button id='remove-item'>X</button></td>
      <td><button id='move-to-grocery-list'>X</button></td>
    </tbody>

  {{/each}}

</table>
